#!/bin/sh

#========================================================================
# find ラッパースクリプト
#========================================================================

#=========================================================================
# findコマンドパス
#=========================================================================
selfpath=$(realpath $0)
systemcmd=$(which -a sort | grep -v "$selfpath" | head -1)

#=========================================================================
# プラットフォームごとに処理を分岐
#=========================================================================
case "$(uname)" in
MSYS*)
	exec /usr/bin/find "$@"
	;;
*)
	exec "$systemcmd" "$@"
	;;
esac
